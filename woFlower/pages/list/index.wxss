/* pages/detail/index.wxss */
image {
  width: 100%;
}
.headerImg>image {
  width: 100%;
  height: 300rpx;
}
.list-wrap, .title {
  padding: 0 15rpx;
}
.title {
  height: 60rpx;
  font-size: 26rpx;
  line-height: 70rpx;
}
.list-wrap {
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-between;
}
.list {
  width: 345rpx;
  margin: 9rpx;
  font-size: 30rpx;
  padding-bottom: 15rpx;
}
.list>image {
  height: 440rpx;
  margin-bottom: 25rpx;
}
.passPrice {
  text-decoration: line-through;
}
.listMess {
  font-size: 23rpx;
}
.passPrice, .listMess {
  color: rgb(194, 194, 194);
}